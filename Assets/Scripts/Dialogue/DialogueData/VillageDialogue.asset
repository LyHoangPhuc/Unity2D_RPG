      characterSprite: {fileID: 252003673, guid: a4b2eff5e4c3e424cb4946481cc54a3c, type: 3}
      displayDuration: 4
      waitForInput: 1
    - characterName: Anna
      dialogueText: "S\u1EED d\u1EE5ng combo c\u1EE7a b\u1EA1n \u0111\u1EC3 g\xE2y
